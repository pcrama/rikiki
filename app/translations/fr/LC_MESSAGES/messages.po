msgstr ""
"Veuillez attendre les autres joueurs et que l'organisateur démarre la "
"partie."

#: player.py:425
#, python-format
msgid " with %(cards)s"
msgstr " avec %(cards)s"

#: player.py:428
msgid "1 trick bid"
msgstr "1 pli annoncé"

#: player.py:431
#, python-format
